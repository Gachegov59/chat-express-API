{
	"useTabs": true,
	"tabWidth": 2,
	"semi": true,
	"singleQuote": true,
	"endOfLine": "auto",
	"printWidth": 110,
	"trailingComma": "es5",
	"overrides": [
		{
			"files": "*.svg",
			"options": {
				"parser": "html"
			}
		},
		{
			"files": "*.json",
			"options": {
				"parser": "json"
			}
		}
	]
}
